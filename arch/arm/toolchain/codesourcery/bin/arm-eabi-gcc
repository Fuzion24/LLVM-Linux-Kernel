#!/bin/bash
export PATH=`dirname $0`/../arm-2011.03/bin:$PATH
`dirname $0`/../../../../../toolchain/clang/install/bin/clang \
	-target arm-gnueabi \
	-ccc-gcc-name arm-none-linux-gnueabi-gcc \
	-D__linux__ -I`dirname $0`/../arm-2011.03/arm-none-linux-gnueabi/libc/usr/include \
	-march=armv7-a --sysroot=`dirname $0`/../arm-2011.03/arm-none-linux-gnueabi/libc $*


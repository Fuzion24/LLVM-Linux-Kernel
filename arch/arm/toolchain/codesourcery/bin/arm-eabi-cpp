#!/bin/sh
export PATH=`dirname $0`/../arm-2011.03/bin:$PATH
`dirname $0`/../../../../toolchain/clang/install/bin/clang \
	-ccc-host-triple arm-eabi \
	-ccc-gcc-name "`dirname $0`/../arm-2011.03/bin/arm-none-linux-gnueabi-gcc" \
	--sysroot=`dirname $0`/../arm-2011.03/arm-none-linux-gnueabi/libc -E  $*
